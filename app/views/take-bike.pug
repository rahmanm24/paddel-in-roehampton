extends layout
block content
    .container  
        .col-xs-12 

        h2 Take your bike
        h2 #{stand.standName}
        p #{stand.location}
        p #{stand.bikes} bikes are available at this stand
        form(action='/authenticate' method='POST')
            label(for='userID') Enter your ID: 
            input('name'='userID') 
            button.btn-success Submit