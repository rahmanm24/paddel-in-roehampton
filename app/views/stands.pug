extends layout
block content
    - console.log(data)
    .container
        .col-xs-12
            h1 Please select your closest bike point from bellow to pick up or return a bike.
            table(border=1)
                for item in data 
                    tr  
                        td 
                            a(href='/selected-stand/' + item.standID ) #{item.standName} - #{item.count} bikes available